{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)-8s %(filename)-11s %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "default"
    }
  },
  "loggers": {
    "main": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "router": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "fetcher": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "analyzer": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "broker": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "ledger": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "strategy": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    }
  }
}
